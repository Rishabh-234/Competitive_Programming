#include<stdio.h>
main()
{
	int i,sum=1,even=0,odd=0;
	int arr[10];
	printf("enter element in array : ");
	for(i=0;i<10;i++)
	{
		scanf("%d",&arr[i]);
		sum=sum+arr[i];
	}
	for(i=0;i<10;i++)
	{
		if(arr[i]%2==0)
		{
			even=even+arr[i];
		}
		else
		{
			odd=odd+arr[i];
		}
	}
	printf("sum of even no. of array : %d\n",even);
	printf("sum of odd no. of array : %d",odd);
}
