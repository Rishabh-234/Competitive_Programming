#include <assert.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(){
    int i,j,a[6][6],max=0;
    for(i=0;i<6;i++)
    {
        for(j=0;j<6;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<4;i++){
        for(j=0;j<4;j++){
            a[i][j]=a[i][j]+a[i][j+1]+a[i][j+2]+a[i+1][j+1]+a[i+2][j]+a[i+2][j+1]+a[i+2][j+2];  
            
        }   
    }
max=a[0][0]; 
for(i=0;i<4;i++){ 
for(j=0;j<4;j++){ 
if(max<=a[i][j]){ 
max=a[i][j];      
}
}
}

 printf("%d",max);
 return 0; 
}
